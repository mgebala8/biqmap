.window{
  width: 80%;
  padding-bottom: 10px;
  background: rgba(255, 255, 255, 0.4);
  //box-shadow: 0px 0px 5px 0px rgba(255, 255, 255, 0.5);
  position: absolute;
  top: 100px;
  left: 50%;
  margin-left: -40%;
  display: none;

  h2{
    margin: 10px;
    padding: 0px;
    width: 100%;
  }

  p{
    margin: 10px;
    padding: 10px;
  }

  .preloader{
    width: 100px;
    height: 100px;
    border: 1px solid red;
    background-size: 50px;
    background-image: url('<%=asset_path 'preloader3.gif' %>');
    background-position: center;
    background-repeat: no-repeat;
  }

  .excel{
    width:100px;
    height:100px;
    border:1px solid red;
    background-size:50px;
    background-image: url('<%=asset_path 'excel.gif' %>');
    background-position: center;
    background-repeat:no-repeat;
  }
}

.button_nav{
  float: right;
  margin: 5px;
  button{
    padding:10px;
  }
}
